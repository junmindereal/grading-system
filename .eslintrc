{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "eslint-config-react-app",
    "eslint-config-prettier"
  ],
  "rules": {
    "strict": ["error", "never"]
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  }
}
